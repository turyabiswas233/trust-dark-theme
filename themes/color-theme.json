{
  "name": "Trust Dark Theme",
  "colors": {
    "editor.background": "#0f141b",
    "editor.foreground": "#b3b3b3",
    "actionBar.toggledBackground": "#f3a125",
    "activityBar.background": "#102020",
    "activityBar.foreground": "#00caca",
    "activityBarBadge.background": "#006781",
    "activityBarBadge.foreground": "#c5f6ff",
    "statusBar.background": "#001718",
    "statusBar.foreground": "#efefef",
    "sideBar.background": "#0f171f",
    "sideBar.foreground": "#96f1f6aa",
    "sideBarTitle.background": "#08110e",
    "sideBarTitle.foreground": "#43eecc",
    "sideBarSectionHeader.background": "#1c2233",
    "menu.background": "#080f11",
    "menu.foreground": "#b2d3cc",
    "menu.border": "#02be80",
    "menu.selectionBackground": "#efe",
    "menu.selectionForeground": "#010",
    "terminal.background": "#0d130e",
    "terminal.foreground": "#aca",
    "terminal.selectionBackground": "#fefefe",
    "terminal.selectionForeground": "#030f07",
    "terminal.ansiBlue": "#03a0f5",
    "terminal.ansiCyan": "#04eedd",
    "terminal.ansiGreen": "#0bf652",
    "terminal.ansiBrightGreen": "#0affaa",
    "editorWidget.background": "#131d1c",
    "editorWidget.foreground": "#66a88d",
    "focusBorder": "#057a4f",
    "editor.selectionBackground": "#053f22a2",
    "minimap.background": "#1a302996",
    "tab.activeBackground": "#103815",
    "tab.activeForeground": "#02f102",
    "tab.inactiveBackground": "#293b3a",
    "tab.inactiveForeground": "#ebf1f0",
    "titleBar.activeBackground": "#0e1116",
    "titleBar.activeForeground": "#a6d1c1",
    "editorGroup.emptyBackground": "#0e0e16",
    "editorGroup.dropBackground": "#00001f55",
    "input.background": "#2b2a2c2a",
    "input.foreground": "#a1aca4",
    "checkbox.background": "#0b110cb4",
    "checkbox.foreground": "#04fa04",
    "scrollbar.shadow": "#4dbb1a2a",
    "scrollbarSlider.background": "#b0fcc355",
    "scrollbarSlider.hoverBackground": "#00ff0d55",
    "scrollbarSlider.activeBackground": "#00ff0d"
  },
  "tokenColors": [
    {
      "name": "Comment",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#417e3fc2"
      }
    },
    {
      "name": "Variables",
      "scope": ["variable", "string constant.other.placeholder"],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#0bfa52"
      }
    },
    {
      "name": "Colors",
      "scope": ["constant.other.color"],
      "settings": {
        "foreground": "#ffffff"
      }
    },
    {
      "name": "Invalid",
      "scope": ["invalid", "invalid.illegal"],
      "settings": {
        "fontStyle": "bold underline",
        "foreground": "#f31b3f"
      }
    },
    {
      "name": "Keyword, Storage",
      "scope": ["keyword", "storage.type", "storage.modifier"],
      "settings": {
        "fontStyle": "",
        "foreground": "#3ab892"
      }
    },
    {
      "name": "Operator, Misc",
      "scope": [
        "keyword.control",
        "constant.other.color",
        "punctuation",
        "meta.tag",
        "punctuation.definition.tag",
        "punctuation.separator.inheritance.php",
        "punctuation.definition.tag.html",
        "punctuation.definition.tag.begin.html",
        "punctuation.definition.tag.end.html",
        "punctuation.section.embedded",
        "keyword.other.template",
        "keyword.other.substitution"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#40f7ba"
      }
    },
    {
      "name": "Tag",
      "scope": [
        "entity.name.tag",
        "meta.tag.sgml",
        "markup.deleted.git_gutter"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#00cf50"
      }
    },
    {
      "name": "Function, Special Method",
      "scope": [
        "entity.name.function",
        "meta.function-call",
        "variable.function",
        "support.function",
        "keyword.other.special-method"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#eef867"
      }
    },
    {
      "name": "Block Level Variables",
      "scope": ["meta.block variable.other"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#53c780"
      }
    },
    {
      "name": "Other Variable, String Link",
      "scope": ["support.other.variable", "string.other.link"],
      "settings": {
        "fontStyle": "italic bold",
        "foreground": "#af393d"
      }
    },
    {
      "name": "Number, Constant, Function Argument, Tag Attribute, Embedded",
      "scope": [
        "constant.numeric",
        "constant.language",
        "support.constant",
        "constant.character",
        "constant.escape",
        "variable.parameter",
        "keyword.other.unit",
        "keyword.other"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#ba75f3"
      }
    },
    {
      "name": "String, Symbol,  Inherited Class, Markup Heading",
      "scope": [
        "string",
        "constant.other.symbol",
        "constant.other.key",
        "entity.other.inherited-class",
        "markup.heading",
        "markup.inserted.git_gutter",
        "meta.group.braces.curly constant.other.object.key.js string.unquoted.label.js"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#d3dbd1"
      }
    },
    {
      "name": "Class, Support",
      "scope": [
        "entity.name",
        "support.type",
        "support.class",
        "support.other.namespace.use.php",
        "meta.use.php",
        "support.other.namespace.php",
        "markup.changed.git_gutter",
        "support.type.sys-types"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#ff5e00"
      }
    },
    {
      "name": "Entity Types",
      "scope": ["support.type"],
      "settings": {
        "foreground": "#84b0c2"
      }
    },
    {
      "name": "CSS Class and Support",
      "scope": [
        "source.css support.type.property-name",
        "source.sass support.type.property-name",
        "source.scss support.type.property-name",
        "source.less support.type.property-name",
        "source.stylus support.type.property-name",
        "source.postcss support.type.property-name"
      ],
      "settings": {
        "foreground": "#759cf1",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Sub-methods",
      "scope": [
        "entity.name.module.js",
        "variable.import.parameter.js",
        "variable.other.class.js"
      ],
      "settings": {
        "foreground": "#FF5370"
      }
    },
    {
      "name": "Language methods",
      "scope": ["variable.language"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#FF5370"
      }
    },
    {
      "name": "entity.name.method.js",
      "scope": ["entity.name.method.js"],
      "settings": {
        "fontStyle": "underline",
        "foreground": "#82AAFF"
      }
    },
    {
      "name": "meta.method.js",
      "scope": [
        "meta.class-method.js entity.name.function.js",
        "variable.function.constructor"
      ],
      "settings": {
        "foreground": "#82AAFF"
      }
    },
    {
      "name": "Attributes",
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#98b497"
      }
    },
    {
      "name": "HTML Attributes",
      "scope": [
        "text.html.basic entity.other.attribute-name.html",
        "text.html.basic entity.other.attribute-name"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#FFCB6B"
      }
    },
    {
      "name": "CSS Classes",
      "scope": ["entity.other.attribute-name.class"],
      "settings": {
        "foreground": "#e9924c"
      }
    },
    {
      "name": "CSS ID's",
      "scope": ["source.sass keyword.control"],
      "settings": {
        "foreground": "#82AAFF"
      }
    },
    {
      "name": "Inserted",
      "scope": ["markup.inserted"],
      "settings": {
        "foreground": "#C3E88D"
      }
    },
    {
      "name": "Deleted",
      "scope": ["markup.deleted"],
      "settings": {
        "foreground": "#FF5370"
      }
    },
    {
      "name": "Changed",
      "scope": ["markup.changed"],
      "settings": {
        "foreground": "#25cf26"
      }
    },
    {
      "name": "Regular Expressions",
      "scope": ["string.regexp"],
      "settings": {
        "foreground": "#89DDFF"
      }
    },
    {
      "name": "Escape Characters",
      "scope": ["constant.character.escape"],
      "settings": {
        "foreground": "#89DDFF"
      }
    },
    {
      "name": "URL",
      "scope": ["*url*", "*link*", "*uri*"],
      "settings": {
        "fontStyle": "underline"
      }
    },
    {
      "name": "Decorators",
      "scope": [
        "tag.decorator.js entity.name.tag.js",
        "tag.decorator.js punctuation.definition.tag.js"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#82AAFF"
      }
    },
    {
      "name": "ES7 Bind Operator",
      "scope": [
        "source.js constant.other.object.key.js string.unquoted.label.js"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#FF5370"
      }
    },
    {
      "name": "JSON Key - Level 0",
      "scope": [
        "source.json meta.structure.dictionary.json support.type.property-name.json"
      ],
      "settings": {
        "foreground": "#25cf26"
      }
    },
    {
